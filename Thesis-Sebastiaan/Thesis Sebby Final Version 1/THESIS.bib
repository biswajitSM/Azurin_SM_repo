@article{And2004,
abstract = {We present a method for the analysis of optical single molecule emission data that exhibit discrete intensity jumps. This new method uses a generalized likelihood ratio test that determines the location of an intensity change point based on individual photon arrival times. This test is applied recursively to an entire single molecule intensity trajectory, thus finding each change points. Expectation−maximization clustering and the Bayesian information criterion is then used for accurate determination of the true number of states accessible to the system. This procedure allows rigorous and quantitative determination of intensity change points without the artificial time resolution limitations that arise from binning and thresholding.},
author = {And, Lucas P. Watkins and Yang*, Haw},
doi = {10.1021/JP0467548},
publisher = { American Chemical Society },
title = {{Detection of Intensity Change Points in Time-Resolved Single-Molecule Measurements}},
year = {2004}
}
@article{Vosch2007,
abstract = {The water-soluble, near-IR-emitting DNA-encapsulated silver nanocluster presented herein exhibits extremely bright and photostable emission on the single-molecule and bulk levels. The photophysics have been elucidated by intensity-dependent correlation analysis and suggest a heavy atom effect of silver that rapidly depopulates an excited dark level before quenching by oxygen, thereby conferring great photostability, very high single-molecule emission rates, and essentially no blinking on experimentally relevant time scales (0.1 to {\textgreater}1,000 ms). Strong antibunching is observed from these biocompatible species, which emit {\textgreater}10(9) photons before photobleaching. The significant dark-state quantum yield even enables bunching from the emissive state to be observed as a dip in the autocorrelation curve with only a single detector as the dark state precludes emission from the emissive level. These species represent significant improvements over existing dyes, and the nonpower law blinking kinetics suggest that these very small species may be alternatives to much larger and strongly intermittent semiconductor quantum dots.},
author = {Vosch, Tom and Antoku, Yasuko and Hsiang, Jung-Cheng and Richards, Chris I and Gonzalez, Jose I and Dickson, Robert M},
doi = {10.1073/pnas.0610677104},
