issn = {1420-3049},
journal = {Molecules (Basel, Switzerland)},
number = {8},
pages = {11660--78},
pmid = {25102116},
title = {{Tracking electrons in biological macromolecules: from ensemble to single molecule.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25102116},
volume = {19},
year = {2014}
}
