title = {{Strongly emissive individual DNA-encapsulated Ag nanoclusters as single-molecule fluorophores.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17519337 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1937515},
